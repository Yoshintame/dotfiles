# Fast edit
alias editzshrc="$EDITOR ~/.zshrc"
alias editaliases="$EDITOR ~/.aliases"
alias edithostfile="sudo $EDITOR /etc/hosts"
alias editssh="sudo $EDITOR ~/.ssh"

# Fast cd
alias dev="cd ~/Development"
alias dotfiles="cd ~/.dotfiles"

# Other
alias dotapply="dotbot -c ~/.dotfiles/install.conf.yaml"
alias dotedit="cursor ~/.dotfiles"
